{
  "name": "freemarker-react",
  "displayName": "FreeMarker React",
  "description": "Syntax highlight for FreeMarker with React",
  "version": "0.0.1",
  "publisher": "sadfghjkljhgfdews",
  "repository": {
    "type": "git",
    "url": "https://github.com/dcortes92/vs-freemarker.git"
  },
  "engines": {
    "vscode": "^0.10.1"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "ftl",
        "aliases": [
          "FreeMarker",
          "ftl"
        ],
        "extensions": [
          ".ftl"
        ],
        "configuration": "./ftl.configuration.json"
      },
      {
        "id": "cchtml"
      }
    ],
    "grammars": [
      {
        "language": "cchtml",
        "scopeName": "text.cchtml.basic",
        "path": "./syntaxes/ccHtml.tmLanguage"
      },
      {
        "language": "ftl",
        "scopeName": "text.html.ftl",
        "path": "./syntaxes/ftl.tmLanguage"
      }
    ],
    "snippets": [
      {
        "language": "ftl",
        "path": "./snippets/ftl.json"
      },
      {
        "language": "ftl",
        "path": "./snippets/html.json"
      }
    ]
  },
  "bugs": {
    "url": "https://github.com/dcortes92/vs-freemarker/issues"
  },
  "homepage": "https://github.com/dcortes92/vs-freemarker#readme",
  "devDependencies": {
    "semantic-release": "^17.3.0",
    "semantic-release-vsce": "^3.0.1"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "publish": "npx semantic-release"
  },
  "keywords": [
    "visual",
    "studio",
    "code",
    "vs",
    "freemarker",
    "ftl"
  ],
  "license": "MIT",
  "icon": "images/logo.svg"
}
